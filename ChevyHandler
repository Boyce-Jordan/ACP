package ACP;

import ACP.AppHandler;

import java.util.HashMap;

public class ChevyHandler implements AppHandler {
    @Override
    public void handleIt(HashMap data)  {

        try {
            System.out.println("You have chosen Chevy!");
            System.out.println("Good Choice!");
            System.out.println("The first Chevy was made in 1913.");
        }catch (Exception e){
            e.printStackTrace();
        }


    }
}
